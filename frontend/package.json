{
  "name": "rsbuild-solid-ts",
  "private": true,
  "version": "1.0.0",
  "scripts": {
    "dev": "rsbuild dev --open --env-dir ../",
    "build": "rsbuild build --env-dir ../",
    "preview": "rsbuild preview --env-dir ../",
    "lint": "biome check ./src/ --apply",
